{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "password",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25,
            "examples": [
                "safe123"
            ]
        },
        "password": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25,
            "examples": [
                "caweofaoewf2"
            ]
        },
        "email": {
            "type": "string",
            "minLength": 1,
            "format": "email",
            "examples": [
                "test@gmail.com"
            ]
        }
    },
    "examples": [{
        "username": "safe123",
        "password": "caweofaoewf2",
        "email": "test@gmail.com"
    }]
}